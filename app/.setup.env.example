# Environment variables for setup_new_project.sh
# Copy this to .setup.env and customize, then run:
#   source .setup.env && ./setup_new_project.sh

# Required: Project name
PROJECT_NAME=my-ai-project

# Optional: Project description
PROJECT_DESCRIPTION="My AI Project Description"

# Optional: Author name
PROJECT_AUTHOR="Your Name"

# Optional: Initial version
PROJECT_VERSION=1.0.0

# Optional: Skip git initialization (set to "true" to skip)
SKIP_GIT_INIT=false

# Optional: Skip .env file creation (set to "true" to skip)
SKIP_ENV_SETUP=false

